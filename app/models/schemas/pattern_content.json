        "type": "object",
        "properties": {
            "note": {
                "type": "string"
            },
            "accent": {
                "type": "boolean"
            },
            "slide": {
                "type": "boolean"
            },
            "up": {
                "type": "boolean"
            },
            "down": {
                "type": "boolean"
            },
            "rest": {
                "type": "boolean"
            }
